<!DOCTYPE html>
<!--[if IE 9 ]><html class="ie ie9 ie-lt10 no-js" lang="en"><![endif]-->
<!--[if gt IE 9]><!-->
<html class="no-js" lang="en">
<!--<![endif]-->
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
<meta name="description" content="<%= @market_page.description %>">
<meta name="keywords" content="<%= @market_page.name %>">
<meta name="author" content="www.tanmer.com">
<title><%= @market_page.name %></title>
<link rel='stylesheet' href='/templetes/market/bone/assets/css/bootstrap.min.css'>
<link rel='stylesheet' href='/templetes/market/bone/assets/css/vendor.css'>
<link rel='stylesheet' href='/templetes/market/bone/assets/css/style.css'>
<link rel='stylesheet' href='/templetes/market/bone/assets/css/color.css'>
<link rel='stylesheet' href='/templetes/market/bone/assets/css/custom.css'>
</head>
<body class="light blank red"> <!-- main color, dark or light -->

  <!-- preloader -->
  <div id="preloader">
    <div id ="preloader-ele" class="ball-scale-ripple-multiple">
      <div></div>
      <div></div>
      <div></div>
    </div>
  </div>
  <!-- /preloader -->

  <div id="bg"></div> <!-- background -->
  <div id="video"> <!-- video -->
    <video id="html5-video" autoplay loop muted>
      <source src="/templetes/market/bone/assets/video/video.mp4" type="video/mp4">
    </video>
  </div>
  <div id="overlay"></div> <!-- overlay -->

  <!-- header -->
  <header id="site-header">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <img src="/templetes/market/bone/assets/img/logo-light.png" alt="logo" class="logo-light"> <!-- light logo -->
          <img src="/templetes/market/bone/assets/img/logo-dark.png" alt="logo" class="logo-dark"> <!-- dark logo -->
          <!-- menu toggle -->
          <div id="menu-toggle">
            <a href="#" title="menu toggle">
              <i class="icons fa fa-bars"></i>
            </a>
          </div>
          <!-- /menu toggle -->
        </div>
      </div>
    </div>
  </header>
  <!-- /header -->

  <!-- menu -->
  <nav id="menu-wrap">
    <div id="menu-inner">
      <ul id="menu">
        <!-- data-link-to for the link section id -->
        <li><a class="active" href="#" data-link-to="#intro">介绍</a></li>
        <li><a href="#" data-link-to="#about">关于</a></li>
        <li><a href="#" data-link-to="#contact">联系</a></li>
        <li><a href="http://www.grand-jardin.com">更多</a></li>
      </ul>
    </div>
  </nav>
  <!-- /menu -->

  <!-- intro section -->
  <section id="intro" class="active">
    <div class="section-wrap">
      <div class="tb">
        <div class="tb-cell">
          <div class="inner">
            <div class="container">
              <div class="row">
                <div class="col-md-12">
                  <div class="row section-heading">
                    <div class="col-md-12">
                      <h1 class="section-title"><span class="letter">Grand </span><span class="color">格兰雅克</span></h1> <!-- section title -->
                      <div class="section-icon"><i class="icons fa fa-diamond"></i></div>  <!-- section icon -->
                      <p class="sub-title">高端衣柜定制服务商.</p> <!-- section subtitle -->
                      <h3><%= @market_page.value_for('summary', title: '活动主题', typo: 'string', default: '0元设计周活动开始了！') %></h3>
                    </div>
                  </div>

                  <div class="row section-footer">
                    <div class="col-md-12">
                      <a data-effect="mfp-zoom-in" href="#subscribe-form-popup" class="btn btn-default btn-lg border shadow">我要免费设计</a> <!-- subscribe button -->
                      <a href="#" class="btn btn-theme btn-lg shadow" data-link-to="#about">活动规则说明</a>
                      <!-- subscribe form-->
                      <div id="subscribe-form-popup" class="mfp-hide white-popup-block mfp-with-anim">
                        <form id="subscribe-form" class="border-form">
                          <h3 class="form-heading">拨打电话，获取免费名额</h3>
                          <div class="form-group">
                            <div class="input-group">
                              <input type="phone" name="phone" id="subscribe-phone" class="form-control input-lg" readonly value="<%= @market_page.value_for('mobile', title: '报名热线', typo: 'string', default: '18080810818') %>">
                              <span class="input-group-btn">
                                <a type="button" class="btn btn-theme btn-lg" href="tel:<%= @market_page.value_for('mobile', title: '报名热线', typo: 'string', default: '18080810818') %>">
                                  马上拨打
                                </a>
                              </span>
                            </div>
                          </div>
                          <label class="subscribe-notice" for="subscribe-email">在线电话预订的客户，可以享受免费设计名额。</label>
                        </form>
                        <% if false %>
                          <% form_for @member, url: agent_frontend_member_create_path([@site,@member]), id: "subscribe-form", class: 'border-form' do %>
                            <h3 class="form-heading">在线预订免费名额</h3>
                            <div class="form-group">
                              <div class="input-group">
                                <input type="hidden" name="member[typo]" value="非正式">
                                <input type="hidden" name="member[from]" value="营销页">
                                <input type="hidden" name="member[owned]" value="营销页：<%= @market_page.id %>">
                                <input type="hidden" name="member[site_id]" value="营销页：<%= @site.id %>">
                                <input type="name" name="member[name]" placeholder="您贵姓" id="subscribe-name" class="form-control input-lg">
                                <input type="phone" name="member[mobile_phone]" placeholder="您的手机号" id="subscribe-phone" class="form-control input-lg">
                                <span class="input-group-btn">
                                  <button type="submit" class="btn btn-theme btn-lg">
                                    发 送
                                  </button>
                                </span>
                              </div>
                            </div>
                            <label class="subscribe-notice" for="subscribe-email">在线预订的客户，可以享受免费设计。</label>
                          <% end %>
                        <% end %>
                      </div>
                      <!-- /subscribe form-->
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <!-- /intro section -->

  <!-- about section -->
  <section id="about">
    <div class="section-wrap">
      <div class="tb">
        <div class="tb-cell">
          <div class="inner">
            <div class="container">
              <div class="row">
                <div class="col-md-12">
                  <div class="row section-heading">
                    <div class="col-md-12">
                      <h1 class="section-title"><span class="letter">活动</span>介 <span class="color">绍</span></h1> <!-- section title -->
                      <div class="section-icon"><i class="icons fa fa-info"></i></div>  <!-- section icon -->
                      <p class="sub-title">the story of our company.</p> <!-- section subtitle -->
                    </div>
                  </div>
                  <div class="row section-content">
                    <!-- services list -->
                    <div id="carousel-services" class="owl-carousel owl-theme">
                      <div class="col-md-12 tl">
                        <p>
                          <%= @market_page.value_for('content', title: '活动详细介绍', typo: 'text', placeholder: '活动规则介绍') %>
                        </p> <!-- about content -->
                      </div>
                      <% @market_page.image_items.each do |img| %>
                         <div class="col-md-12">
                           <div class="services-grid">
                             <%= image_tag img.image_url, class: 'img-responsive' %>
                           </div>
                        </div>
                      <% end %>
                    </div>
                  </div>
                  <div class="row section-footer">
                    <div class="col-md-12">
                      <a href="#" class="btn btn-theme btn-lg shadow" data-link-to="#contact">联系方式</a>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <!-- /about section -->

  <!-- services section -->

  <!-- /services section -->

  <!-- contact section -->
  <section id="contact">
    <div class="section-wrap">
      <div class="tb">
        <div class="tb-cell">
          <div class="inner">
            <div class="container">
              <div class="row">
                <div class="col-md-12">
                  <div class="row section-heading">
                    <div class="col-md-12">
                      <h1 class="section-title"><span class="letter">联系</span> <span class="color">格兰</span>.</h1> <!-- section title -->
                      <div class="section-icon"><i class="icons fa fa-send-o"></i></div>  <!-- section icon -->
                      <p class="sub-title">get in touch with us.</p> <!-- section subtitle -->
                    </div>
                  </div>
                  <div class="row section-content">
                    <!-- contact info -->
                    <div class="contact-info">
                      <div class="col-sm-4">
                        <span class="icons-wrap">
                          <i class="icons icon-phone"></i>
                        </span>
                        <span class="contact-way"><%= @market_page.value_for('mobile', title: '联系电话', typo: 'string', required: true) %></span>
                      </div>
                      <div class="col-sm-4">
                        <span class="icons-wrap">
                          <i class="icons icon-envelope"></i>
                        </span>
                        <span class="contact-way">QQ: <%= @market_page.value_for('contact_qq', title: 'QQ', typo: 'string') %></span>
                      </div>
                      <div class="col-sm-4">
                        <span class="icons-wrap">
                          <i class="icons icon-map-pin"></i>
                        </span>
                        <span class="contact-way"><%= @market_page.value_for('address', title: '公司地址', typo: 'string', default: '崇州市工业园') %></span>
                      </div>
                    </div>
                    <!-- /contact info -->
                    <div class="col-md-12">
                      <!-- contact form -->

                      <!-- /contact form -->
                    </div>
                  </div>
                  <div class="row section-footer">
                    <div class="col-md-12">
                      <a href="#" class="btn btn-theme btn-lg shadow" data-link-to="#intro">返回首页</a>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <!-- /contact section -->

  <!-- footer -->
  <footer id="site-footer">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div class="copyright">
            <a href="http://cms.dagle.cn" title="copyright" target="_blank">@2017 德格软件</a>
          </div>
          <!-- social -->
          <!-- /social -->
        </div>
      </div>
    </div>
  </footer>
  <!-- /footer -->

  <!-- script -->
  <script src='/templetes/market/bone/assets/js/vendor/jquery-1.11.3.min.js'></script>
  <script src='/templetes/market/bone/assets/js/vendor/bootstrap.min.js'></script>
  <script src='/templetes/market/bone/assets/js/vendor/plugin.js'></script>
  <!--[if lt IE 10]>
  <script src='/templetes/market/bone/assets/js/vendor/jquery.placeholder.min.js'></script>
  <![endif]-->
  <script src='/templetes/market/bone/assets/js/static.js'></script>
  <script src='/templetes/market/bone/assets/js/main.js'></script>
  <script src='/templetes/market/bone/assets/js/custom.js'></script>
  <!-- /script -->

</body>

