<!-- Header -->
<header style="background-image: url(/templetes/app-landing-tomoko/img/bg/big-images/1.jpg)" data-stellar-background-ratio="0.3">
  <!-- Intro -->
  <div id="intro" class="overlay-gradient" data-scroll-index="0">
    <!-- Top Bar -->
    <div class="top-bar">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <div class="logo">
              <a title="" href="/" data-toggle="tooltip" data-placement="top" >
                <img src="/templetes/app-landing-tomoko/logo.png" width="160px" height="60px"/>
              </a>
            </div>
            <div class="nav">
              <nav class="navbar navbar-default" role="navigation">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                <span class="sr-only">菜单</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                </button>
                <ul class="collapse navbar-collapse navbar-ex1-collapse">
                  <li><a data-scroll-nav="0" title="" href="/">首页</a></li>
                  <li><a data-scroll-nav="1" title="">产品介绍</a></li>
                  <li><a data-scroll-nav="2" title="">功能特点</a></li>
                  <li><a data-scroll-nav="4" title="">软件预览</a></li>
                  <li><a data-scroll-nav="6" title="">商家入驻</a></li>
                  <li><a href="/news" title="">新闻中心</a></li>
                  <li class="download-btn"><a data-scroll-nav="3" title="">APP下载 <i class="fa fa-cloud-download"></i></a></li>
                </ul>
              </nav>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- Top Bar End -->
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <!-- Intro Heading -->
          <div class="headline">
            <img src="<%=@share_obj.headshot %>" alt="">
            <p>
              <p>我是<%= @share_obj.nickname %></p>
              邀请你注册淘美客
            </p>

            <p class="wow fadeInDown" data-wow-delay="0.5s">
              淘美客是一家提供在线美容预约服务的私享平台，专业打造优质舒适的社区美容服务，丰富多样的美容项目、各有所长的美容技师供您自由选择。</p>
          </div>
          <!-- Intro Heading End -->
          <!-- Phone Carousel -->
          <div class="download">
            <ul class="dw-btns">
              <li><a data-toggle="tooltip" data-placement="top" title="" href="<%= @cms_site.value_for('ios')%>" data-original-title="下载iPhone版"><i class="fa fa-apple fa-lg"></i></a></li>
              <li><a data-toggle="tooltip" data-placement="top" title="" href="<%= @cms_site.value_for('android')%>" data-original-title="下载Android版"><i class="fa fa-android fa-lg"></i></a></li>
            </ul>
          </div>

          <!-- Phone Carousel End -->
        </div>
      </div>
      <!-- .row End -->
    </div>
    <!-- .container End -->
  </div>
  <!-- Intro End -->
</header>
<!-- Header -->
