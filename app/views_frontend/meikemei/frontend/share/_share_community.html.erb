            <div class="logo"><a title="" href="#"><img src="/templetes/app-landing-tomoko/logo.png" width="160px" height="60px"/></a></div>
            <!-- Logo End -->
            <!-- Menu -->
            <div class="nav">
              <nav class="navbar navbar-default" role="navigation">
                <!-- Mobile Button -->
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
