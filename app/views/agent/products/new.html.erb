<!-- BEGIN #checkout-cart -->
<div class="section-container" id="products_new">
    <!-- BEGIN container -->
    <div class="container">
        <!-- BEGIN checkout -->
        <div class="checkout">
          <!-- BEGIN checkout-header -->
          <div class="checkout-header">
              <!-- BEGIN row -->
              <div class="row">
                  <!-- BEGIN col-3 -->
                  <div class="col-md-3 col-sm-3">
                      <div class="step active">
                          <a href="#basicInformation" role="tab" data-toggle="tab">
                            <div class="number">1</div>
                            <div class="info">
                                <div class="title">商品基本信息</div>
                                <div class="desc"> Basic information of commodities</div>
                            </div>
                          </a>
                      </div>
                  </div>
                  <!-- END col-3 -->
                  <!-- BEGIN col-3 -->
                  <div class="col-md-3 col-sm-3">
                      <div class="step">
                        <a href="#configurationInformation" role="tab" data-toggle="tab">
                          <div class="number">2</div>
                          <div class="info">
                              <div class="title">商品配置信息</div>
                              <div class="desc">Product configuration information.</div>
                          </div>
                        </a>
                      </div>
                  </div>
                  <!-- END col-3 -->
                  <!-- BEGIN col-3 -->
                  <div class="col-md-3 col-sm-3">
                      <div class="step">
                        <a href="#LogisticsInformation" role="tab" data-toggle="tab">
                          <div class="number">3</div>
                          <div class="info">
                            <div class="title">物流配置</div>
                            <div class="desc">Logistics distribution. </div>
                          </div>
                        </a>
                      </div>
                  </div>
                  <!-- END col-3 -->
                  <!-- BEGIN col-3 -->
                  <div class="col-md-3 col-sm-3">
                      <div class="step">
                        <a href="#last" role="tab" data-toggle="tab">
                          <div class="number">4</div>
                          <div class="info">
                            <div class="title">确认发布</div>
                            <div class="desc">Product updates.</div>
                          </div>
                        </a>
                      </div>
                  </div>
                  <!-- END col-3 -->
              </div>
              <!-- END row -->
          </div>
          <!-- END checkout-header -->

          <!-- Tab panes -->
          <div class="tab-content">
            <div role="tabpanel" class="tab-pane fade in active" id="basicInformation">
              <!-- BEGIN checkout-body -->
              <div class="checkout-body">
                <form role="form">
                  <div class="form-group col-xs-4">
                    <label for="exampleInputEmail1">商品名称</label>
                    <input type="email" class="form-control" placeholder=" ">
                  </div>
                  <div class="form-group col-xs-4">
                    <label for="exampleInputPassword1">商品单价 </label>
                    <input type="number" class="form-control" placeholder="">
                  </div>
                  <div class="form-group col-xs-4">
                    <label for="exampleInputFile">商品库存</label>
                    <input type="number" class="form-control" placeholder="">
                  </div>
                  <div class="col-xs-12">
                    <div class="form-group">
                      <!-- [1,2] 'imagepath selected-ids="[1,2]"-->
                      <label for="exampleInputFile">商品主图</label>
                      <image-select name="paroducts[image_items_ids]" server=""  style="min-height:150px"></image-select>
                      <span class="small help-block">请上传多张图片，以备使用</sapn>
                    </div>
                  </div>
                </form>
              </div>
              <!-- END checkout-body -->
              <!-- BEGIN checkout-footer -->
              <div class="checkout-footer">
                <a href="#configurationInformation" role="tab" data-toggle="tab" class="btn btn-inverse btn-lg p-l-30 p-r-30 m-l-10">下一步</a>
              </div>
              <!-- END checkout-footer -->
            </div>
            <div role="tabpanel" class="tab-pane fade" id="configurationInformation">
              <!-- BEGIN checkout-body -->
              <div class="checkout-body">
                <form role="form">

                  <div class="form-group col-xs-12">
                    <label for="exampleInputEmail1">商品描述<span class="lable">1</span></label>
                    <textarea class="form-control" rows="5">
                    </textarea>
                  </div>
                </form>
              </div>
              <!-- END checkout-body -->
              <!-- BEGIN checkout-footer -->
              <div class="checkout-footer">
                  <!-- <a href="#" class="btn btn-white btn-lg pull-left">Continue Shopping</a> -->
                  <button type="submit" class="btn btn-inverse btn-lg p-l-30 p-r-30 m-l-10">下一步</button>
              </div>
              <!-- END checkout-footer -->
            </div>
            <div role="tabpanel" class="tab-pane fade" id="LogisticsInformation">
              <!-- BEGIN checkout-body -->
              <div class="checkout-body">
                <form role="form">
                  <div class="form-group col-xs-12">
                    <label for="exampleInputEmail1">商品库存计数方式</label>
                    <div class="">
                      <label class="radio-inline">
                        <input type="radio" name="inlineRadioOptions" id="inlineRadio1" value="option1"> 下单减库存
                      </label>
                      <label class="radio-inline">
                        <input type="radio" name="inlineRadioOptions" id="inlineRadio2" value="option2"> 实际付款减库存
                      </label>
                    </div>
                  </div>
                  <div class="form-group col-xs-12">
                    <label for="exampleInputPassword1">商品运费</label>
                    <input type="password" class="form-control" placeholder="">
                  </div>
                  <div class="form-group col-xs-12">
                    <label for="exampleInputFile">发货地址</label>
                    <input type="password" class="form-control" placeholder="">
                  </div>
                  <div class="form-group col-xs-12">
                    <label for="exampleInputEmail1">发票</label>
                    <div class="">
                      <label class="radio-inline">
                        <input type="radio" name="inlineRadioOptions-1" id="inlineRadio3" value="option1"> 无
                      </label>
                      <label class="radio-inline">
                        <input type="radio" name="inlineRadioOptions-1" id="inlineRadio4" value="option2"> 有
                      </label>
                    </div>
                  </div>
                  <div class="form-group col-xs-12">
                    <label for="exampleInputEmail1">保修</label>
                    <div class="">
                      <label class="radio-inline">
                        <input type="radio" name="inlineRadioOptions-2" id="inlineRadio5" value="option1"> 无
                      </label>
                      <label class="radio-inline">
                        <input type="radio" name="inlineRadioOptions-2" id="inlineRadio6" value="option2"> 有
                      </label>
                    </div>
                  </div>
                </form>
              </div>
              <!-- END checkout-body -->
              <!-- BEGIN checkout-footer -->
              <div class="checkout-footer">
                  <!-- <a href="#" class="btn btn-white btn-lg pull-left">Continue Shopping</a> -->
                  <button type="submit" class="btn btn-inverse btn-lg p-l-30 p-r-30 m-l-10">下一步</button>
              </div>
              <!-- END checkout-footer -->
            </div>
            <div role="tabpanel" class="tab-pane fade" id="last">
              <!-- BEGIN checkout-body -->
              <div class="checkout-body">
                <label class="h5" for="">阅读同意书：</label>
                <textarea class="form-control" rows="20" readonly="true">

                  CoffeeScript 是一门编译到 JavaScript 的小巧语言. 在 Java 般笨拙的外表下, JavaScript 其实有着一颗华丽的心脏. CoffeeScript 尝试用简洁的方式展示 JavaScript 优秀的部分.

                </textarea>
                <div class="form-group text-center p-t-10">
                  <label class="radio-inline">
                    <input type="radio" name="inlineRadioOptions" id="inlineRadio1" value="option1"> 同意
                  </label>
                  <label class="radio-inline">
                    <input type="radio" name="inlineRadioOptions" id="inlineRadio2" value="option2"> 不同意
                  </label>
                </div>
              </div>
              <!-- END checkout-body -->

              <!-- BEGIN checkout-footer -->
              <div class="checkout-footer text-center">
                <button type="submit" class="btn btn-white btn-lg p-l-30 p-r-30 m-l-10">发布商品</button>
              </div>
              <!-- END checkout-footer -->
            </div>
          </div>
          <!-- End Tab panes -->





        </div>
        <!-- END checkout -->
    </div>
    <!-- END container -->
</div>
