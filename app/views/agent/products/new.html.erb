<!-- BEGIN #checkout-cart -->
<div class="section-container">
    <!-- BEGIN container -->
    <div class="container">
        <!-- BEGIN checkout -->
        <div class="checkout">
          <!-- BEGIN checkout-header -->
          <div class="checkout-header">
              <!-- BEGIN row -->
              <div class="row">
                  <!-- BEGIN col-3 -->
                  <div class="col-md-3 col-sm-3">
                      <div class="step active">
                          <a href="#basicInformation" role="tab" data-toggle="tab">
                            <div class="number">1</div>
                            <div class="info">
                                <div class="title">商品基本信息</div>
                                <div class="desc"> Basic information of commodities</div>
                            </div>
                          </a>
                      </div>
                  </div>
                  <!-- END col-3 -->
                  <!-- BEGIN col-3 -->
                  <div class="col-md-3 col-sm-3">
                      <div class="step">
                        <a href="#configurationInformation" role="tab" data-toggle="tab">
                          <div class="number">2</div>
                          <div class="info">
                              <div class="title">商品配置信息</div>
                              <div class="desc">Product configuration information.</div>
                          </div>
                        </a>
                      </div>
                  </div>
                  <!-- END col-3 -->
                  <!-- BEGIN col-3 -->
                  <div class="col-md-3 col-sm-3">
                      <div class="step">
                        <a href="#LogisticsInformation" role="tab" data-toggle="tab">
                          <div class="number">3</div>
                          <div class="info">
                            <div class="title">物流配置</div>
                            <div class="desc">Logistics distribution. </div>
                          </div>
                        </a>
                      </div>
                  </div>
                  <!-- END col-3 -->
                  <!-- BEGIN col-3 -->
                  <div class="col-md-3 col-sm-3">
                      <div class="step">
                        <a href="#last" role="tab" data-toggle="tab">
                          <div class="number">4</div>
                          <div class="info">
                            <div class="title">预览/发布</div>
                            <div class="desc">Preview/Product updates.</div>
                          </div>
                        </a>
                      </div>
                  </div>
                  <!-- END col-3 -->
              </div>
              <!-- END row -->
          </div>
          <!-- END checkout-header -->

          <!-- Tab panes -->
          <div class="tab-content">
            <div role="tabpanel" class="tab-pane fade in active" id="basicInformation">
              <!-- BEGIN checkout-body -->
              <div class="checkout-body">
                <form role="form">
                  <div class="form-group">
                    <label for="exampleInputEmail1">Email address</label>
                    <input type="email" class="form-control" id="exampleInputEmail1" placeholder="Enter email">
                  </div>
                  <div class="form-group">
                    <label for="exampleInputPassword1">Password</label>
                    <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password">
                  </div>
                  <div class="form-group">
                    <label for="exampleInputFile">File input</label>
                    <input type="file" id="exampleInputFile">
                    <p class="help-block">Example block-level help text here.</p>
                  </div>
                  <div class="checkbox">
                    <label>
                      <input type="checkbox"> Check me out
                    </label>
                  </div>
                  <button type="submit" class="btn btn-default">Submit</button>
                </form>
              </div>
              <!-- END checkout-body -->
              <!-- BEGIN checkout-footer -->
              <div class="checkout-footer">
                  <!-- <a href="#" class="btn btn-white btn-lg pull-left">Continue Shopping</a> -->
                  <button type="submit" class="btn btn-inverse btn-lg p-l-30 p-r-30 m-l-10">下一步</button>
              </div>
              <!-- END checkout-footer -->
            </div>
            <div role="tabpanel" class="tab-pane fade" id="configurationInformation">
              <!-- BEGIN checkout-body -->
              <div class="checkout-body">
                <h1>configurationInformation</h1>
              </div>
              <!-- END checkout-body -->
              <!-- BEGIN checkout-footer -->
              <div class="checkout-footer">
                  <!-- <a href="#" class="btn btn-white btn-lg pull-left">Continue Shopping</a> -->
                  <button type="submit" class="btn btn-inverse btn-lg p-l-30 p-r-30 m-l-10">下一步</button>
              </div>
              <!-- END checkout-footer -->
            </div>
            <div role="tabpanel" class="tab-pane fade" id="LogisticsInformation">
              <!-- BEGIN checkout-body -->
              <div class="checkout-body">
                <h1>LogisticsInformation</h1>
              </div>
              <!-- END checkout-body -->
              <!-- BEGIN checkout-footer -->
              <div class="checkout-footer">
                  <!-- <a href="#" class="btn btn-white btn-lg pull-left">Continue Shopping</a> -->
                  <button type="submit" class="btn btn-inverse btn-lg p-l-30 p-r-30 m-l-10">下一步</button>
              </div>
              <!-- END checkout-footer -->
            </div>
            <div role="tabpanel" class="tab-pane fade" id="last">
              <!-- BEGIN checkout-body -->
              <div class="checkout-body">
                <h1>last</h1>
              </div>
              <!-- END checkout-body -->

              <!-- BEGIN checkout-footer -->
              <div class="checkout-footer text-center">
                <button type="submit" class="btn btn-white btn-lg p-l-30 p-r-30 m-l-10">发布商品</button>
              </div>
              <!-- END checkout-footer -->
            </div>
          </div>
          <!-- End Tab panes -->





        </div>
        <!-- END checkout -->
    </div>
    <!-- END container -->
</div>
